<%= form_with(model: @supplier) do |f| %>
  <div>
    <%= f.label :company_name  %>
    <%= f.text_field :company_name %>
  </div>

  <div>
    <%= f.label :company_register %>
    <%= f.text_field :company_register %>
  </div>

  <div>
    <%= f.label :brand_name %>
    <%= f.text_field :brand_name %>
  </div>

  <div>
    <%= f.label :adress %>
    <%= f.text_field :adress %>
  </div>

<div>
    <%= f.label :city %>
    <%= f.text_field :city %>
  </div>

  <div>
    <%= f.label :state %>
    <%= f.text_field :state %>
  </div>

  <div>
    <%= f.label :email %>
    <%= f.text_field :email %>
  </div>

  <div>
    <%= f.label :phone_number %>
    <%= f.text_field :phone_number %>
  </div>

  <div> 
    <%= f.submit 'Cadastrar' %>
  </div>

<% end %>